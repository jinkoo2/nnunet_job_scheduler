[project]
name = "nnunet-jobs-handler"
version = "0.2.0"
description = ""
authors = [
    {name = "Jinkoo Kim"}
]
license = {text = "MIT"}
readme = "README.md"
requires-python = ">=3.9"
dependencies = [
    "simple-slurm-server @ git+https://github.com/jinkoo2/simple-slurm-server.git@main"
]

[project.scripts]
main = "nnunet_job_scheduler.app:main"
pp_main = "nnunet_job_scheduler.pp:main"

[build-system]
requires = ["poetry-core>=2.0.0,<3.0.0"]
build-backend = "poetry.core.masonry.api"

[tool.poetry]
name = "nnunet-jobs-handler"
version = "0.1.0"
description = ""
authors = [{ name = "Jinkoo Kim" }]
license = "MIT"
readme = "README.md"
packages = [
    { include = "nnunet_job_scheduler", from = "src" }
]
# Other tool.poetry.* options stay here

[tool.poetry.dependencies]
python = ">=3.9"
simple-slurm-server = { git = "https://github.com/jinkoo2/simple-slurm-server.git", branch = "main" }

[tool.poetry.scripts]
main = "nnunet_job_scheduler.app:main"
pp_main = "nnunet_job_scheduler.pp:main"

[build-system]
requires = ["poetry-core>=2.0.0,<3.0.0"]
build-backend = "poetry.core.masonry.api"
